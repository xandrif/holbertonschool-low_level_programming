#include "holberton.h"

/**
 * add - prints the last digit.
(*
 * @n: this is a parameter
 * @m: this is a parameter
 * @j: this is a parameter
 * Description: returns absolute values for specified signs
(* section header: Section description)*
 * Return: 0 (Success)
 */

int add(int n, int m)
{
	int j;

	j = (n + m);

	return (j);
}
