#include "holberton.h"

/**
 *  _isupper - returns 1 if something is uppercase
 * @c: an integer
(*
 * Description: Longer description of the function)?
(* section header: Section description)*
* Return: 0
*/

int _isupper(int c)
{
	if (c >= 65 && c <= 90)
	{
		return (1);
	}

	else
	{
		return (0);
	}
}
