int prime_factor(int n)
{
	int j = 0;

	while (j < n)
		if ((n % j) == 0)
			{
				int l;
				int k = 0;

				l = (n / j);
				while (k < l )
				{
					(2 ** k);
					l = (l * k);
					if ((n % l) == 0)
					{
						k++;
					}
					else
					{
						printf("%d", l);
					}
				}
				else if ((n % j) = 0)
				{
					j++;
				}
