#include "holberton.h"

/**
 * int _isupper - Short description, single line
 * @c: an integer
(*
 * Description: Longer description of the function)?
(* section header: Section description)*
* Return: 0
*/

int _isupper(int c)
{
	if (c >= 48 && c <= 58)
	{
		return (1);
	}

	else
	{
		return (0);
	}
}
